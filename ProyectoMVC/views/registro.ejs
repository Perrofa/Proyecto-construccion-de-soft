<html>
    <head>
        <link
          href="https://unpkg.com/gridjs/dist/theme/mermaid.min.css"
          rel="stylesheet"
        />
    </head>
    <body>
        <h2 class="title"> Registro </h2>
        <form action="/usuarios/registro" method="POST">
            <p>
                <label class="label" for="id">ID</label>
                <input class="input text" id="id" name="id">
            </p>
            
            <p>
                <label class="label" for="nombre">Nombre de usuario</label>
                <input class="input text" id="nombre" name="nombre">
            </p>
            
            <p>
                <label class="label" for="pass">Contrase√±a</label>
                <input class="input text" id="pass" name="pass">
            </p>

            <p>
                <label class="label" for="mail">Correo</label>
                <input class="input text" id="mail" name="mail">
            </p>
            
            <p>
                <label class="label" for="priv">Privilegio</label>
                <input class="input text" id="priv" name="priv">
            </p>

            <input class="button is-success" type="submit" value="registrar">
        </form>
        <div id="wrapper"></div>
    </body>
    <script src="https://cdn.jsdelivr.net/npm/gridjs/dist/gridjs.umd.js"></script>
    <%- include('./script.ejs') %>
</html>